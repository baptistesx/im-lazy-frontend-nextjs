		"standard-version": "^9.3.2",
		"typescript": "4.6.2",
		"validate-branch-name": "^1.3.0"
	},
	"validate-branch-name": {
		"pattern": "^(main|develop){1}$|^(feature|bugfix|hotfix|chore|experiment|config)/.+$",
		"errorMsg": "Branch name format : <type>/<name>"
	},
	"lint-staged": {
		"*.(js|html|css|ts|tsx)": [
			"npx prettier --write",
			"npx eslint --fix"
		],
		"public/*/*.json": [
			"npx prettier --write"
		]
	}
}
